// 1. Cap the Global Resources (This fixes your warning)
params {
    max_cpus   = 16
    max_memory = '250.GB'
    max_time   = '72.h'
}

// 2. Process-Specific Resources
process {
    withName: 'PICARD_MARKDUPLICATES' {
        memory = 200.GB
        time   = 24.h
        queue  = 'prip'
        clusterOptions = '-q primary'
    }
}
